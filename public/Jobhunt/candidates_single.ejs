<!DOCTYPE html>
<%- include('includes/header', data); %>
<body>
<%- include('includes/loader', data); %>
<div class="theme-layout" id="scrollup">

	<%- include('includes/dashboard_responsive_header', data); %>
	<%- include('includes/dashboard_responsive_header__loggedin_employeer', data); %>

	<section class="overlape">
		<div class="block no-padding">
			<div data-velocity="-.1" style="background: url(http://placehold.it/1600x800) repeat scroll 50% 422.28px transparent;" class="parallax scrolly-invisible no-parallax"></div><!-- PARALLAX BACKGROUND IMAGE -->
			<div class="container fluid">
				<div class="row">
					<div class="col-lg-12">
						<div class="inner-header">
							<div class="container">
								<div class="row">
									<div class="col-lg-6">
										<div class="skills-btn">
											<a href="#" title="">Photoshop</a>
											<a href="#" title="">Designers</a>
											<a href="#" title="">Illustrator</a>

										</div>
									</div>
									<div class="col-lg-6">
										<div class="action-inner">
											<a href="#" title=""><i class="la la-paper-plane"></i>Save Resume</a>
											<a href="#" title=""><i class="la la-envelope-o"></i>Contact <%= data.fullName %></a>
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</section>

	<section class="overlape">
		<div class="block remove-top">
			<div class="container">
				<div class="row">
					<div class="col-lg-12">
						<div class="cand-single-user">
							<div class="share-bar circle">
								<a href="#" title="" class="share-google"><i class="la la-google"></i></a><a href="#" title="" class="share-fb"><i class="fa fa-facebook"></i></a><a href="#" title="" class="share-twitter"><i class="fa fa-twitter"></i></a>
							</div>
							<div class="can-detail-s">
								<div class="cst"><img src="http://placehold.it/145x145" alt="" /></div>
								<h3><%= data.resume.basics.name %></h3>
								<span><i><%= data.resume.basics.label %></i> </span>
								<p><%= data.resume.basics.email  %>  | <%= data.resume.basics.phone  %>  | <a href="<%= data.resume.basics.website %>" target="_blank" ><%= data.resume.basics.website  %></a>  </p>
								<% if(data.resume.created_at) { %><p>Member Since, <%= data.resume.created_at %></p> <% } %>
								<p><i class="la la-map-marker"></i> <%= data.resume.basics.location.city %> / <%= data.resume.basics.location.region %></p>
							</div>
							<div class="download-cv">
								<a href="#" title="">Download CV <i class="la la-download"></i></a>
							</div>
						</div>
						<ul class="cand-extralink">
							<li><a href="#about" title="About">About</a></li>
							<li><a href="#education" title="Education">Education</a></li>
							<li><a href="#experience" title="Work Experience">Work Experience</a></li>
							<li><a href="#volunteering" title="Volunteering">Volunteering</a></li>
							<li><a href="#awards" title="Awards Skills">Awards Skills</a></li>
							<li><a href="#publications" title="Publications">Publications</a></li>
							<li><a href="#skills" title="Languages">Skills</a></li>
							<li><a href="#languages" title="Languages">Languages</a></li>
							<li><a href="#interests" title="Interests">Interests</a></li>
							<li><a href="#references" title="References">References</a></li>
						</ul>
						<div class="cand-details-sec">
							<div class="row">
								<div class="col-lg-8 column">
									<div class="cand-details" id="about">
										<h2>Candidates About</h2>
										<p><%= data.description %> </p>
										<div class="edu-history-sec" id="education">
											<h2>Education</h2>

											<% for(let i=0; i < data.resume.education.length; i++){ %>
												<div class="edu-history">
													<i class="la la-graduation-cap"></i>
													<div class="edu-hisinfo">
														<h3><%= data.resume.education[i].institution %></h3>
														<i> <%= data.resume.education[i].startDate %> -  <%= data.resume.education[i].endDate %>  </i>
														<span> <%= data.resume.education[i].gpa %> GPA in <i> <%= data.resume.education[i].studyType %> of <%= data.resume.education[i].area %> </i> </span>
														<p>Cources Completed:</p>
														<ol style="padding-left: 30px">
															<% for ( let j = 0; j <= data.resume.education[i].courses.length; j ++ ) { %>
																<% if ( data.resume.education[i].courses[j]) { %>
																	<li><small> <%-  data.resume.education[i].courses[j]  %></small> </li>
																<% } %>
															<% } %>
														</ol>
													</div>
												</div>
											<%}%>
										</div>
										<div class="edu-history-sec" id="experience">
											<h2>Work & Experience</h2>
											<% for(let i = 0; i < data.resume.work.length; i++ ){ %>
												<div class="edu-history style2">
													<i></i>
													<div class="edu-hisinfo">
														<h3><%= data.resume.work[i].position %> <span> AT  <%= data.resume.work[i].company %></span></h3>
														<a href="<%= data.resume.work[i].website %>" target="_blank"><%= data.resume.work[i].website %></a>
														<i><%= data.resume.work[i].startDate %> TO <%= data.resume.work[i].endDate %></i>
														<p><%= data.resume.work[i].summary %></p>

														<p>Highlights:</p>
														<ol style="padding-left: 30px">
															<% for ( let j = 0; j <= data.resume.work[i].highlights.length; j ++ ) { %>
																<% if ( data.resume.work[i].highlights[j]) { %>
																	<li><small> <%-  data.resume.work[i].highlights[j]  %></small> </li>
																<% } %>
															<% } %>
														</ol>
													</div>
												</div>
											<% } %>
										</div>

										<div class="edu-history-sec" id="volunteering">
											<h2>Volunteering Experience</h2>
											<% for(let i = 0; i < data.resume.volunteer.length; i++ ){ %>
												<div class="edu-history style2">
													<i></i>
													<div class="edu-hisinfo">
														<h3><%= data.resume.volunteer[i].organization %> <span> AS  <%= data.resume.volunteer[i].position %></span></h3>
														<a href="<%= data.resume.volunteer[i].website %>" target="_blank"><%= data.resume.volunteer[i].website %></a>
														<i><%= data.resume.volunteer[i].startDate %> TO <%= data.resume.volunteer[i].endDate %></i>
														<p><%= data.resume.volunteer[i].summary %></p>
														<p>Highlights:</p>
														<ol style="padding-left: 30px">
															<% for ( let j = 0; j <= data.resume.volunteer[i].highlights.length; j ++ ) { %>
																<% if ( data.resume.volunteer[i].highlights[j]) { %>
																	<li><small> <%-  data.resume.volunteer[i].highlights[j]  %></small> </li>
																<% } %>
															<% } %>
														</ol>
													</div>
												</div>
											<% } %>
										</div>


										<div class="edu-history-sec" id="awards">
											<h2>Awards</h2>
											<% for(let i = 0; i < data.resume.awards.length; i++ ){ %>
												<div class="edu-history style2">
													<i></i>
													<div class="edu-hisinfo">
														<h3><%= data.resume.awards[i].title %> </h3>
														<a href="<%= data.resume.awards[i].website %>" target="_blank"><%= data.resume.awards[i].website %></a>
														<i><%= data.resume.awards[i].date %> </i>
														<p><%= data.resume.awards[i].awarder %></p>
														<p><%= data.resume.awards[i].summary %></p>
													</div>
												</div>
											<% } %>
										</div>

										<div class="edu-history-sec" id="skills">
											<h2>Skills</h2>
											<% for(let i = 0; i < data.resume.skills.length; i++ ){ %>
												<div class="edu-history style2">
													<div class="edu-hisinfo">
														<h5><%= data.resume.skills[i].name %></h5>
														<ol style="padding-left: 30px">
															<% for ( let j = 0; j <= data.resume.skills[i].keywords.length; j ++ ) { %>
																<% if ( data.resume.skills[i].keywords[j]) { %>
																	<li><small> <%-  data.resume.skills[i].keywords[j]  %></small> </li>
																<% } %>
															<% } %>
														</ol>
													</div>
												</div>
											<% } %>
										</div>


										<div class="edu-history-sec" id="languages">
											<h2>Languages</h2>
											<% for(let i = 0; i < data.resume.languages.length; i++ ){ %>
												<div class="edu-history style2">
													<div class="edu-hisinfo">
														<h3><%= data.resume.languages[i].language %><span>(LEVEL : <%= data.resume.languages[i].fluency %>)</span></h3>
													</div>
												</div>
											<% } %>
										</div>

										<div class="edu-history-sec" id="interests">
											<h2>Interests</h2>
											<% for(let i = 0; i < data.resume.interests.length; i++ ){ %>
												<div class="edu-history style2">
													<div class="edu-hisinfo">
														<h5><%= data.resume.interests[i].name %></h5>
														<ol style="padding-left: 30px">
															<% for ( let j = 0; j <= data.resume.skills[i].keywords.length; j ++ ) { %>
																<% if ( data.resume.skills[i].keywords[j]) { %>
																	<li><small> <%-  data.resume.skills[i].keywords[j]  %></small> </li>
																<% } %>
															<% } %>
														</ol>
													</div>
												</div>
											<% } %>
										</div>

										<div class="edu-history-sec" id="references">
											<h2>References</h2>
											<% for(let i = 0; i < data.resume.references.length; i++ ){ %>
												<div class="edu-history style2">
													<div class="edu-hisinfo">
														<h3> - <%= data.resume.references[i].name %></h3>
														<blockquote>
															<%= data.resume.references[i].reference %></blockquote>
													</div>
												</div>
											<% } %>
										</div>


										<!-------------------------------------------------------------------------------------------------------------------------------------->
										<div class="companyies-fol-sec">
											<h2>Companies Followed By</h2>
											<div class="cmp-follow">
												<div class="row">
													<div class="col-lg-2 col-md-2 col-sm-3 col-xs-12">
														<a href="#" title=""><img src="http://placehold.it/80x80" alt="" /><span>King LLC</span></a>
													</div>
													<div class="col-lg-2 col-md-2 col-sm-3 col-xs-12">
														<a href="#" title=""><img src="http://placehold.it/80x80" alt="" /><span>Telimed</span></a>
													</div>
													<div class="col-lg-2 col-md-2 col-sm-3 col-xs-12">
														<a href="#" title=""><img src="http://placehold.it/80x80" alt="" /><span>Ucesas</span></a>
													</div>
													<div class="col-lg-2 col-md-2 col-sm-3 col-xs-12">
														<a href="#" title=""><img src="http://placehold.it/80x80" alt="" /><span>TeraPlaner</span></a>
													</div>
													<div class="col-lg-2 col-md-2 col-sm-3 col-xs-12">
														<a href="#" title=""><img src="http://placehold.it/80x80" alt="" /><span>Cubico</span></a>
													</div>
													<div class="col-lg-2 col-md-2 col-sm-3 col-xs-12">
														<a href="#" title=""><img src="http://placehold.it/80x80" alt="" /><span>Airbnb</span></a>
													</div>
												</div>
											</div>
										</div>
									</div>
								</div>
								<div class="col-lg-4 column">
									<div class="job-overview">
										<h3>Job Overview</h3>
										<ul>
											<li><i class="la la-money"></i><h3>Offerd Salary</h3><span><%=data.currentSalaryMin%> - <%data.currentSalaryMax%></span></li>
											<li><i class="la la-mars-double"></i><h3>Gender</h3><span>Female</span></li>
											<li><i class="la la-thumb-tack"></i><h3>Career Level</h3><span><%=data.jobTitle%></span></li>
											<li><i class="la la-puzzle-piece"></i><h3>Industry</h3><span><%=data.JobTitle%></span></li>
											<li><i class="la la-shield"></i><h3>Experience</h3><span><%=data.experience%> Years</span></li>
											<li><i class="la la-line-chart "></i><h3>Qualification</h3><span><%=data.educationlevel%></span></li>
										</ul>
									</div><!-- Job Overview -->
									<div class="quick-form-job">
										<h3>Contact</h3>
										<form action="mailAction" method="post">
											<input type="text" required placeholder="Enter your Name *" />
											<input type="text" required placeholder="Email Address*" />
											<input type="text" required placeholder="Phone Number" />
											<textarea required placeholder="Message should have more than 50 characters"></textarea>
											<button class="submit">Send Email</button>
											<span>You accepts our <a href="#" title="">Terms and Conditions</a></span>
										</form>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</section>

